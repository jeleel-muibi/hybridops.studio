# Generated by rke2_install role
{% if rke2_type == 'server' %}
write-kubeconfig-mode: "0644"
tls-san:
  - {{ ansible_fqdn }}
{% endif %}
{% if rke2_type == 'agent' %}
server: "{{ rke2_server_url }}"
token: "{{ rke2_token }}"
{% endif %}
{% for k, v in rke2_config_extra.items() %}
{{ k }}: {{ v }}
{% endfor %}
